<div class="dialogBox" id="alertMessageDialog">
	<div class="popupHeader" id="modal_header">
		Alert <span class="dialog_close" id="close"
			onclick="document.getElementById('alertMessageDialog').style.display='none'">
			<i class="fa fa-close"></i>
	</div>
	<div id="dialog_text"></div>
</div>

